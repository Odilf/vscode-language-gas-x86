{
    "name": "language-gas-x86",
    "displayName": "gas",
    "description": "GNU Assembler x86 language support",
    "version": "0.0.1",
    "publisher": "basdp",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "gas",
            "aliases": ["GAS/AT&T x86/x64", "gas"],
            "extensions": [".s",".S",".sx"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "gas",
            "scopeName": "source.x86",
            "path": "./syntaxes/gas.tmLanguage"
        }]
    }
}